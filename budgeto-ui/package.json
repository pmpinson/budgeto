{
  "name": "budgeto-ui",
  "description": "Web UI for budgeto",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/pmpinson/budgeto",
  "license": "",
  "dependencies": {
    "angular": "^1.4.7",
    "angular-resource": "^1.4.7",
    "angular-ui-bootstrap": "^0.14.3",
    "angular-ui-router": "^0.2.15",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.5",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.21.0",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.2",
    "less-loader": "^2.2.1",
    "less-middleware": "1.0.x",
    "lodash": "^3.10.1",
    "morgan": "~1.6.1",
    "postcss-loader": "^0.7.0",
    "raw-loader": "^0.5.1",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.4.1"
  },
  "devDependencies": {
    "//": {
      "protractor": "^1.1.1",
      "shelljs": "^0.2.6"
    },
    "cssmin ": "latest",
    "extract-text-webpack-plugin": "^0.8.2",
    "glob": "latest",
    "htmlmin": "latest",
    "http-server": "latest",
    "jasmine-reporters": "latest",
    "jscs": "^2.3.5",
    "jshint": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-cli": "latest",
    "karma-coverage": "latest",
    "karma-firefox-launcher": "latest",
    "karma-html-reporter": "latest",
    "karma-jasmine": "latest",
    "karma-junit-reporter": "latest",
    "karma-phantomjs-launcher": "latest",
    "karma-requirejs": "latest",
    "less": "latest",
    "live-reload": "latest",
    "mkdirp": "latest",
    "opener": "latest",
    "parallelshell": "latest",
    "protractor": "latest",
    "rimraf": "latest",
    "uglify-js": "latest",
    "watch": "latest",
    "webdriver-manager": "latest",
    "yargs": "latest"
  },
  "scripts": {
    "validate:html": "echo nothing",
    "build:html": "echo nothing",
    "dist:html": "mkdirp target/dist/app && node config/build/htmlmin.js app/**/*.html:target/dist",
    "validate:style": "lessc -l app/styles/styles.less",
    "build:style": "lessc app/styles/styles.less > app/styles/styles.css",
    "watch:style": "watch \"npm run build:style\" app/styles",
    "dist:style": "mkdirp target/dist/app/styles && lessc app/styles/styles.less | cssmin > target/dist/app/styles/styles.css",
    "validate:script": "jshint --config config/.jshintrc --exclude src/main/client/lib/ app",
    "build:script": "echo nothing",
    "watch:script": "watch \"npm run build:script\" app",
    "dist:script": "mkdirp target/dist/app && jshint --config config/.jshintrc --exclude src/main/client/lib/ app && node config/build/uglify.js app/*.js:target/dist app/components/**/*.js:target/dist -e src/main/client/lib/*",
    "validate:image": "echo nothing",
    "build:image": "echo nothing",
    "dist:image": "mkdirp target/dist/app/images && node config/build/images.js app/**/*.gif:target/dist",
    "build": "npm run build:html && npm run build:style && npm run build:script && npm run build:image",
    "watch": "parallelshell \"npm run watch:style\" \"npm run watch:script\"",
    "dist": "rimraf target && npm run build && npm run test:dist && mkdirp target && rimraf target/bower_components && npm run dist:html && npm run dist:style && npm run dist:script && npm run dist:image",
    "test:validate": "jshint --config config/.jshintrc-test test",
    "test:single": "rimraf target/coverage && rimraf target/test-results && karma start config/karma.conf.js --single-run",
    "test:dist": "rimraf target/coverage && rimraf target/test-results && karma start config/karma.dist.conf.js --single-run",
    "test:watch": "rimraf target/coverage && rimraf target/test-results && karma start config/karma.conf.js",
    "test": "npm run test:watch",
    "e2etest:validate": "jshint --config config/.jshintrc-e2e-test e2e-tests",
    "pree2etest:single": "webdriver-manager update",
    "e2etest:single": "protractor config/protractor.conf.js",
    "dev": "parallelshell \"npm run build:serve\" \"npm run watch\"",
    "live-reload": "live-reload --port 8999 app/",
    "build:serve": "npm run build && npm run open-browser & http-server -a localhost -p 9000 -c-1",
    "dist:serve": "npm run dist && npm run open-browser & http-server target/dist/ -a localhost -p 9000 -c-1",
    "start": "npm run dist:serve",
    "expressjsstart": "node ./bin/www",
    "new:install:dev": "npm install",
    "new:install:prod": "npm install --production",
    "new:test:single": "karma start config/karma.conf.js --single-run",
    "new:start:dev": "parallelshell \"babel-node src/main/server.js\" \"opener http://localhost:3000\""
  }
}
